# Kernel extra targets to build
  KERNEL_UBOOT_EXTRA_TARGET="meson8_M8Nb_1G_emmc_sdhc.dtd meson8_M8Nb_1G_emmc_sdhc.dtb meson8_M8Nb_1G_emmc_sdio.dtd meson8_M8Nb_1G_emmc_sdio.dtb meson8_M8Nb_2G_emmc_sdhc.dtd meson8_M8Nb_2G_emmc_sdhc.dtb meson8_M8Nb_2G_emmc_sdio.dtd meson8_M8Nb_2G_emmc_sdio.dtb meson8m2_M8C_1G.dtd meson8m2_M8C_1G.dtb meson8m2_M8S_2G.dtd meson8m2_M8S_2G.dtb meson8m2_n200C_1G.dtd meson8m2_n200C_1G.dtb meson8m2_n200C_2G.dtd meson8m2_n200C_2G.dtb"

# Build Android kernel image using mkbootimg
  BUILD_ANDROID_BOOTIMG="yes"

# Path to a file that will be passed as the --second parameter to Android mkbootimg
  ANDROID_BOOTIMG_SECOND="out/dt.img"

# OpenGL-ES implementation to use (no / bcm2835-driver / opengl-meson6 / opengl-meson8)
  OPENGLES="opengl-meson8"

  MULTI_DTBS="yes"